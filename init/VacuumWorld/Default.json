{

@possibleactions : {
	communicationaction : uk.ac.rhul.cs.dice.starworlds.actions.environmental.CommunicationAction,
	sensingaction       : uk.ac.rhul.cs.dice.starworlds.actions.environmental.SensingAction,
	moveaction          : uk.ac.rhul.cs.dice.starworlds.experiment.physicalagents.MoveAction,
},

@environments : {
	universe : uk.ac.rhul.cs.dice.starworlds.experiment.physicalagents.PhysicalEnvironment
},

@appearances : {
	environmentalappearance : uk.ac.rhul.cs.dice.starworlds.appearances.EnvironmentalAppearance,
},

@physics : {
	physics : uk.ac.rhul.cs.dice.starworlds.experiment.physicalagents.PhysicalPhysics,
},

@states : {
	state : uk.ac.rhul.cs.dice.starworlds.experiment.physicalagents.PhysicalState,
},

@agentminds : {
	mind : uk.ac.rhul.cs.dice.starworlds.experiment.physicalagents.RandomPhysicalAgentMind,
},

@agentbodies : {
	agent : uk.ac.rhul.cs.dice.starworlds.entities.agents.concrete.DefaultAgent,
},

@agentsensors : {
	badseeingsensor : uk.ac.rhul.cs.dice.starworlds.experiment.physicalagents.BadSeeingSensor,
	listeningsensor : uk.ac.rhul.cs.dice.starworlds.entities.agents.components.concrete.ListeningSensor,
},

@agentactuators : {
	physicalactuator : uk.ac.rhul.cs.dice.starworlds.entities.agents.components.concrete.PhysicalActuator,
	speechactuator   : uk.ac.rhul.cs.dice.starworlds.entities.agents.components.concrete.SpeechActuator,
},

~agents : {
	PhysicalAgent : {
		@agentbodies    : agent,
		@agentminds     : mind,
		@agentsensors   : [listeningsensor, badseeingsensor],
		@agentactuators : [physicalactuator, speechactuator],
	},
},

~environments : {
	Universe : {
		@environments 	 : universe,
		@possibleactions : [communicationaction, sensingaction, moveaction],
		@physics         : physics,
		@states          : state,
	},
},

~structure : [{
		~environments : Universe,
		~agents : [PhysicalAgent, PhysicalAgent, PhysicalAgent],
	},
]
}




